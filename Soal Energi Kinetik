def energi_kinetik(kecepatan, massa):
    energi_kinetik = 0.5 * massa * kecepatan**2
    return energi_kinetik

# Input
kecepatan_benda = 10  # dalam m/s
massa_benda_2 = 2     # dalam kilogram

# Hitung dan cetak energi kinetik
energi_kinetik_benda = energi_kinetik(kecepatan_benda, massa_benda_2)
print("Energi kinetik benda adalah", energi_kinetik_benda, "Joule.")
